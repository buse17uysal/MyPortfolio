@model List<MyPortfolio.DAL.Entites.Contact>

    <div class="col-four tab-full contact__infos">
    @foreach (var item in Model)
    {
        <h4 class="h06">Telefon</h4>
        <p>
            @item.Phone

        </p>

        <h4 class="h06">Email</h4>
        <p>
            @item.Email
        </p>

        <h4 class="h06">Adres</h4>
        <p>
            @item.Address <br />
            17600
        </p>
    }

</div>